library(ggplot2)
scores_data <- data.frame(
  Group = c("X", "X", "X", "X", "Y", "Y", "Y", "Y"),
  Score = c(80, 85, 78, 92, 88, 76, 80, 84)
)
ggplot(scores_data, aes(x = Group, y = Score, fill = Group)) +
  geom_violin(trim = FALSE, alpha = 0.6) +  # Violin plot with transparency
  geom_jitter(width = 0.1, size = 2, color = "black") + 
  labs(
    title = "Violin Plot of Scores by Group",
    x = "Group",
    y = "Score"
  ) +
  theme_minimal()  
